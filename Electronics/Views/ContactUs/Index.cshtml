@model IEnumerable<Electronics.Models.ContactU>

@{
    ViewData["Title"] = "Index";
}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

<h4>Users Message</h4>
<head>
 <style>
.btn.btn-secondary {
    color: #ffffff;
    background-color: #6b6b6b;
    border-color: #6b6b6b;}
.btn {
    display: inline-block;
    font-weight: 400;
    line-height: 0.5;
    color: #212529;
    text-align: center;
    text-decoration: none;
    vertical-align: middle;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    user-select: none;
    background-color: transparent;
    border: 0px solid transparent;
    padding: 0.375rem 0.75rem;
    font-size: 1rem;
    border-radius: 0.25rem;
    transition: color .15s ease-in-out,background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;}
.table thead tr th {
    font-size: 1.063rem;
    font-weight: bold;}
</style>
</head>
@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
<table class="table">
    <thead>
        <tr>
            <th>
               First Name
            </th>
            <th>
               Last Name
            </th>
            <th>
               Phone 
            </th>
            <th>
              Message
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
        @*    <td>
                @Html.DisplayFor(modelItem => item.Web.WebId)
            </td>*@
            <td>
                @*<a asp-action="Edit" asp-route-id="@item.ContactUsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ContactUsId">Details</a> |*@
                <a asp-action="Delete" asp-route-id="@item.ContactUsId" class="btn btn-secondary">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
